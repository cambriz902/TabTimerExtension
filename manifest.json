{
  "manifest_version": 2,
  "name": "tab-timer",
  "description": "keeps track of time a tab has been active",
  "version" : "1.0",
  "browser_action" : {
    "default_icon": "icon.png",
    "default_popup" : "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions" : ["activeTab", "tabs"]
}
